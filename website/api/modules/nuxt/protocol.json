{
	"frameworkName": "Nuxt",
	"customTypesToMerge": ["page"],
	"pathToMergedSlices": "slices.json",
	"prismicConfig": "prismic.config.js",
	"examplesFolder": "pages",
	"firstCommand": "npm run dev",
	"additionalDisplay": "Before running your project, make sure you added\n`modules: [\n\t['@nuxtjs/prismic', {\n\t\tendpoint: 'https://<REPOSITORY>.cdn.prismic.io/api/v2',\n\t\troutes: [\n\t\t'/pages/',\n\t\t'/pages/:uid',\n\t\t'/',\n\t]}\n\t]\n]\n\nto your Nuxt config file",
	"projectTests": [
		{
			"arg": "-f",
			"path": "nuxt.config.js",
			"reason": "No `nuxt.config.js` file found"
		},
		{
			"arg": "-d",
			"path": "pages",
			"reason": "No `pages` folder found"
		}
	],
	"bootstraper": ["npx", ["create-nuxt-app"]],
	"actions": [
		{
			"type": "write",
			"zipPath": "plugins/link-resolver.js",
			"overwrite": false
		},
		{
			"zipPath": "plugins/prismic-vue.js",
			"overwrite": false
		},
		{
			"zipPath": "pages/pages/_uid.vue",
			"overwrite": false
		},
		{
			"zipPath": "pages/index.vue",
			"overwrite": false,
			"bootstrapped": true
		},
		{
			"zipPath": "custom_types/",
			"overwrite": false,
			"bootstrapped": true
		},
		{
			"type": "delete",
			"zipPath": "custom_types/"
		},
		{
			"type": "delete",
			"zipPath": "pages/"
		},
		{
			"type": "delete",
			"zipPath": "plugins/"
		}
	],
	"dependencies": ["prismic-javascript", "prismic-vue", "@nuxtjs/prismic"],
	"devDependencies": ["node-sass", "sass-loader"]
}
